  "license": "MIT",
  "dependencies": {
    "@vscode/sqlite3": "^5.0.8",
    "better-sqlite3": "^7.5.1",
    "chalk": "^5.3.0",
    "change-case": "^5.1.2",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
